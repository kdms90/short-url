                                                        <th data-field="descr">{{ 'Domaine'|trans({}, 'feature') }}</th>
                                                        <th data-field="descr">{{ 'end_date'|trans({}, 'external') }}</th>
                                                        <th data-field="descr">{{ 'Budget'|trans({}, 'feature') }}</th>
                                                        <th data-field="descr">{{ 'Pays'|trans({}, 'feature') }}</th>
                                                    </tr>
                                                    </thead>
                                                    <tbody>
                                                    {% for item in lastProjects %}
                                                        <tr>
                                                            <td>
                                                                <a href="{{ path('app_feature_back_project_detail', {'id':item.id}) }}">
                                                                    {{ item.i18n(locale).name }}
                                                                </a>
                                                                <br>
                                                                <small>{{ 'created_on'|trans({}, 'external') }} {{ item.dateAdd|localizeddate }}</small>
                                                            </td>
                                                            <td>
                                                                {% if item.domain is not null %}
                                                                    {{ item.domain.i18n(locale).name }}
                                                                {% else %}
                                                                    -
                                                                {% endif %}
                                                            </td>
                                                            <td>{{ item.targetEndDate|localizeddate }}</td>
                                                            <td>{{ item.budget|localizedcurrency(currency,locale) }}</td>
                                                            <td>
                                                                {% if item.country is not null %}
                                                                    {{ item.country.i18n(locale).name }}
                                                                {% endif %}
                                                            </td>
                                                        </tr>
                                                    {% endfor %}
                                                    </tbody>
                                                </table>
                                                <!-- end project list -->
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
