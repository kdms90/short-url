{% extends 'foundation/back/back.html.twig' %}

{% block title %}Hello IndexController!{% endblock %}
{% block internalClass %}dashboad{% endblock %}
{% block back %}
    <div class="c">
        <div class="row clearfix">
            <div class="col-lg-3 col-md-6 col-sm-12">
                <a href="{{ path('app_offers_back_offers') }}" class="widget">
                    <div  class="widget-body">
                        <div class="d-flex justify-content-between align-items-center">
                            <div class="state">
                                <h6>{{ 'Toutes mes candidatures'|trans({}, 'core') }}</h6>
                                <p class="count">{{ offersCount|localizednumber('decimal') }}</p>
                            </div>
                            <div class="icon">
                                <i class="ik ik-clock"></i>
                            </div>
                        </div>
                    </div>
                    <div class="progress progress-sm">
                        <div class="progress-bar bg-blue" role="progressbar" aria-valuenow="100" aria-valuemin="0"
                             aria-valuemax="100" style="width: 100%;"></div>
                    </div>
                </a>
            </div>
            <div class="col-lg-3 col-md-6 col-sm-12">
                <a href="{{ path('app_offers_back_applications') }}" class="widget">
                    <div class="widget-body">
                        <div class="d-flex justify-content-between align-items-center">
                            <div class="state">
                                <h6>Candidatures retenues</h6>
                                <p class="count">{{ applicationsCount|localizednumber('decimal') }}</p>
                            </div>
                            <div class="icon">
                                <i class="ik ik-box"></i>
                            </div>
                        </div>
                    </div>
                    <div class="progress progress-sm">
                        <div class="progress-bar bg-green" role="progressbar" aria-valuenow="100" aria-valuemin="0"
                             aria-valuemax="100" style="width: 100%;"></div>
                    </div>
                </a>
            </div>
            <div class="col-lg-3 col-md-6 col-sm-12">
                <a href="{{ path('app_feature_back_projects') }}" class="widget">
                    <div class="widget-body">
                        <div class="d-flex justify-content-between align-items-center">
                            <div class="state">
                                <h6>Mes favoris</h6>
                                <p class="count">{{ projectsCount|localizednumber('decimal') }}</p>
                            </div>
                            <div class="icon">
                                <i class="ik ik-disc"></i>
                            </div>
                        </div>
                    </div>
                    <div class="progress progress-sm">
                        <div class="progress-bar bg-primary" role="progressbar" aria-valuenow="100" aria-valuemin="0"
                             aria-valuemax="100" style="width: 100%;"></div>
                    </div>
                </a>
            </div>
            <div class="col-lg-3 col-md-6 col-sm-12">
                <a href="{{ path('app_user_back_accounts') }}" class="widget">
                    <div class="widget-body">
                        <div class="d-flex justify-content-between align-items-center">
                            <div class="state">
                                <h6>Comptes</h6>
                                <p class="count">{{ accountsCount|localizednumber('decimal') }}</p>
                            </div>
                            <div class="icon">
                                <i class="ik ik-users"></i>
                            </div>
                        </div>
                    </div>
                    <div class="progress progress-sm">
                        <div class="progress-bar bg-info" role="progressbar" aria-valuenow="100" aria-valuemin="0"
                             aria-valuemax="100" style="width: 100%;"></div>
                    </div>
                </a>
            </div>
        </div>
        <div class="row">
            <div class="col-md-8">
                <div class="card">
                    <div class="card-body">
                        <div class="row align-items-center">
                            <div class="col-lg-12 col-md-12">
                                <div class="pt-10">
                                    <h3 class="card-title">Dernières candidatures<span class="more-back-link-content float-right"><a href="{{ path('app_offers_back_offers') }}">+ d'opportunités</a></span></h3>
                                    <div id="advanced_table_wrapper" class="dataTables_wrapper dt-bootstrap4 no-footer">
                                        <div class="row">
                                            <div class="col-sm-12">
                                                <!-- start project list -->
                                                <table class="table dataTable no-footer dtr-inline">
                                                    <thead>
                                                    <tr class="headings">
                                                        <th data-field="name" style="width: 400px;">Titre de lappel</th>
                                                        <th>Emetteur</th>
                                                        <th data-field="name">Budget</th>
                                                        <th data-field="descr">Fin</th>
                                                        <th data-field="candidatures">Candidatures</th>
                                                    </tr>
                                                    </thead>
                                                    <tbody>
                                                    {% for item in lastOffers %}
                                                        <tr>
                                                            <td>
                                                                <a href="{{ path('app_offers_back_offers_detail', {'id':item.id}) }}">
                                                                    {{ item.i18n(locale).name }}
                                                                </a>
                                                                <br>
                                                                <small>{{ 'created_on'|trans({}, 'external') }} {{ item.dateAdd|localizeddate }}</small>
                                                            </td>
                                                            <td>{{ item.transmitter }}</td>
                                                            {% if is_granted('CAN_VIEW_OFFERS_BUDGET') %}
                                                                <td>{{ item.budget|localizedcurrency(currency,locale) }}</td>
                                                            {% endif %}
                                                            <td>{{ item.endDate|localizeddate('medium','none') }}</td>
                                                            <td>
                                                                {% if item.applications|length == 0 %}
                                                                    {{ 'Pas de candidatures'|trans({}, 'opportunities') }}
                                                                {% else %}
                                                                    <a href="#"
                                                                       class="view-related">{{ item.applications|length|localizednumber('decimal') }}
                                                                        {{ 'candidature(s)'|trans({}, 'opportunities') }}</a>
                                                                {% endif %}
                                                            </td>
                                                        </tr>
                                                    {% endfor %}
                                                    </tbody>
                                                </table>
                                                <!-- end project list -->
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="card">
                    <div class="card-body">
                        <div class="row align-items-center">
                            <div class="col-lg-12 col-md-12">
                                <div class="pt-10">
                                    <h3 class="card-title">Opportunités à candidater <span class="more-back-link-content float-right"><a href="{{ path('app_feature_back_projects') }}">+ de projets</a></span></h3>
